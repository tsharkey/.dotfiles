pathadd "$HOME/dev/promoboxx/pbxx-scripts"
export GOSUMDB=off

alias jwt-prod='aws-vault exec pbxx-prod -- go-jwt -type system -duration 3600 -keyProvider paramStore -paramStoreEnv production'
alias jwt-stg='aws-vault exec pbxx-stg -- go-jwt -type system -duration 3600 -keyProvider paramStore -paramStoreEnv staging'
alias jwt-dev='aws-vault exec pbxx-dev -- go-jwt -type system -duration 3600 -keyProvider paramStore -paramStoreEnv development'
alias jwt-internal='aws-vault exec pbxx-internal -- go-jwt -type system -duration 3600 -keyProvider paramStore -paramStoreEnv internal'

alias deploy-configs='code ~/dev/promoboxx/deploy-config-dev ~/dev/promoboxx/deploy-config-stg ~/dev/promoboxx/deploy-config-prod'
